{
	"id": "discord-rpc-hide-nsfw",
	"name": "Discord RPC Hide NSFW",
	"version": "1.0.1",
	"manifestURI": "https://raw.githubusercontent.com/nnotwen/n-seanime-extensions/master/plugins/Discord%20RPC%20Hide%20NSFW/discord-rpc-hide-nsfw.json",
	"language": "typescript",
	"type": "plugin",
	"description": "Hide NSFW entries from appearing in Discord Rich Presence",
	"author": "nnotwen",
	"icon": "https://raw.githubusercontent.com/nnotwen/n-seanime-extensions/master/plugins/Discord%20RPC%20Hide%20NSFW/icon.png",
	"website": "https://github.com/nnotwen/n-seanime-extensions",
	"lang": "en",
	"payloadURI": "https://raw.githubusercontent.com/nnotwen/n-seanime-extensions/master/plugins/Discord%20RPC%20Hide%20NSFW/provider.ts",
	"plugin": {
		"version": "1",
		"permissions": {
			"scopes": ["anilist"]
		}
	},
	"userConfig": {
		"requiresConfig": true,
		"version": 1,
		"fields": [
			{
				"name": "ecchi",
				"label": "Hide ecchi (hentai is hidden by default)",
				"type": "switch",
				"default": "true"
			}
		]
	}
}
