{
	"id": "shikimorisync",
	"name": "ShikimoriSync",
	"version": "1.0.0",
	"manifestURI": "https://raw.githubusercontent.com/nnotwen/n-seanime-extensions/master/plugins/ShikimoriSync/shikimorisync.json",
	"language": "typescript",
	"type": "plugin",
	"description": "An example plugin",
	"author": "nnotwen",
	"icon": "https://shikimori.one/favicons/favicon-96x96.png",
	"website": "https://github.com/nnotwen/n-seanime-extensions",
	"lang": "multi",
	"payloadURI": "https://raw.githubusercontent.com/nnotwen/n-seanime-extensions/master/plugins/ShikimoriSync/provider.ts",
	"plugin": {
		"version": "1",
		"permissions": {
			"scopes": ["storage", "anilist"]
		}
	},
	"userConfig": {
		"requiresConfig": true,
		"version": 1,
		"fields": [
			{
				"name": "authToken",
				"label": "Shikimori Auth Token",
				"type": "text",
				"default": ""
			}
		]
	}
}
